{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "app-generic-new": {
      "root": "",
      "sourceRoot": "web",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "web/index.html",
            "main": "web/main.ts",
            "polyfills": "web/polyfills.ts",
            "tsConfig": "web/tsconfig.app.json",
            "assets": [
              { "glob": "**/*", "input": "./node_modules/@signature-it/ngx-catalogue/web/assets/images", "output": "./assets/images/" },
              { "glob": "**/*", "input": "./node_modules/@signature-it/ngx-generic/web/assets", "output": "./assets/"},
              { "glob": "**/*", "input": "./node_modules/delta-angular/src/fonts", "output": "./fonts/" },
              { "glob": "**/*", "input": "fonts", "output": "./fonts/"},
              { "glob": "**/*", "input": "assets", "output": "./assets/" },
              "web/.htaccess"
            ],
            "styles": [
              "web/styles/inc/_fonts.scss",
              "web/styles/inc/_colors.scss",
              "web/styles/inc/_variables.scss",
              "web/styles/vendors.imports.scss",
              "web/styles/main.overrides.scss",
              "web/styles/main.scss",
              "web/styles/main.rtl.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "./node_modules/delta-angular/src/styles",
                "./node_modules/@signature-it/ngx-generic/web/styles",
                "web/styles"
              ]
            },
            "scripts": [
              "./node_modules/delta-angular/bower_components/jquery-1.9.1/index.js",
              "./node_modules/delta-angular/bower_components/tether/dist/js/tether.min.js",
              "./node_modules/delta-angular/bower_components/bootstrap/dist/js/bootstrap.min.js",
              "./node_modules/delta-angular/bower_components/lodash/dist/lodash.min.js",
              "./node_modules/delta-angular/bower_components/js-storage/js.storage.min.js",
              "./node_modules/delta-angular/bower_components/moment/moment.js",
              "./node_modules/delta-angular/bower_components/PACE/pace.min.js",
              "./node_modules/delta-angular/bower_components/sweetalert2/dist/sweetalert2.min.js",
              "./node_modules/delta-angular/bower_components/toastr/toastr.min.js",
              "./node_modules/delta-angular/bower_components/approvejs/dist/approve.min.js",
              "./node_modules/delta-angular/bower_components/nouislider/distribute/nouislider.min.js",
              "./node_modules/delta-angular/bower_components/jquery-treegrid/js/jquery.treegrid.min.js",
              "./node_modules/delta-angular/bower_components/bootstrap-datepicker/dist/js/bootstrap-datepicker.min.js",
              "./node_modules/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.concat.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "app-generic-new:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "app-generic-new:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "app-generic-new:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "app-generic-new-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "app-generic-new:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "app-generic-new:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "app-generic-new"
}
