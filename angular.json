{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "app-generic-new": {
      "root": "",
      "sourceRoot": "web",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/app-generic-new",
            "index": "web/index.html",
            "main": "web/main.ts",
            "polyfills": "web/polyfills.ts",
            "tsConfig": "web/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "./node_modules/delta-angular/src/fonts",
                "output": "./fonts/"
              },
              {
                "glob": "**/*",
                "input": "fonts",
                "output": "./fonts/"
              },
              {
                "glob": "**/*",
                "input": "web/assets",
                "output": "./assets/"
              },
              "web/.htaccess"
            ],
            "styles": [
              "web/styles/inc/_index.scss",
              "web/styles/vendors.imports.scss",
              "web/styles/vendors.overrides.scss",
              "web/styles/elements/index.scss",
              "web/styles/main.scss",
              "web/styles/main.rtl.scss",
              "node_modules/typeface-exo/index.css",
              "node_modules/roboto-fontface/css/roboto/roboto-fontface.css",
              "node_modules/ionicons/scss/ionicons.scss",
              "node_modules/@fortawesome/fontawesome-free/css/all.css",
              "node_modules/socicon/css/socicon.css",
              "node_modules/nebular-icons/scss/nebular-icons.scss",
              "node_modules/pace-js/templates/pace-theme-flash.tmpl.css",
              "node_modules/leaflet/dist/leaflet.css",
              "projects/signature-it/ngx-admin/src/lib/@theme/styles/styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "./node_modules/delta-angular/src/styles",
                "web/styles"
              ]
            },
            "scripts": [
              "./node_modules/delta-angular/bower_components/jquery-1.9.1/index.js",
              "./node_modules/delta-angular/bower_components/tether/dist/js/tether.min.js",
              "./node_modules/bootstrap/dist/js/bootstrap.bundle.min.js",
              "./node_modules/delta-angular/bower_components/lodash/dist/lodash.min.js",
              "./node_modules/delta-angular/bower_components/js-storage/js.storage.min.js",
              "./node_modules/delta-angular/bower_components/moment/moment.js",
              "./node_modules/delta-angular/bower_components/PACE/pace.min.js",
              "./node_modules/delta-angular/bower_components/sweetalert2/dist/sweetalert2.min.js",
              "./node_modules/delta-angular/bower_components/toastr/toastr.min.js",
              "./node_modules/delta-angular/bower_components/approvejs/dist/approve.min.js",
              "./node_modules/delta-angular/bower_components/nouislider/distribute/nouislider.min.js",
              "./node_modules/delta-angular/bower_components/jquery-treegrid/js/jquery.treegrid.min.js",
              "./node_modules/delta-angular/bower_components/bootstrap-datepicker/dist/js/bootstrap-datepicker.min.js",
              "./node_modules/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.concat.min.js",
              "node_modules/pace-js/pace.min.js",
              "node_modules/tinymce/tinymce.min.js",
              "node_modules/tinymce/themes/modern/theme.min.js",
              "node_modules/tinymce/plugins/link/plugin.min.js",
              "node_modules/tinymce/plugins/paste/plugin.min.js",
              "node_modules/tinymce/plugins/table/plugin.min.js",
              "node_modules/echarts/dist/echarts.min.js",
              "node_modules/echarts/dist/extension/bmap.min.js",
              "node_modules/chart.js/dist/Chart.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "web/environments/environment.ts",
                  "with": "web/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "8mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "app-generic-new:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "app-generic-new:build:production"
            }
          }
        },

        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "app-generic-new:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "web/test.ts",
            "polyfills": "web/polyfills.ts",
            "tsConfig": "web/tsconfig.spec.json",
            "karmaConfig": "web/karma.conf.js",
            "styles": [
              "web/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "web/favicon.ico",
              "web/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "web/tsconfig.app.json",
              "web/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "app-generic-new-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "app-generic-new:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "app-generic-new:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "@signature-it/ngx-generic": {
      "root": "projects/signature-it/ngx-generic",
      "sourceRoot": "projects/signature-it/ngx-generic/src",
      "projectType": "library",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/signature-it/ngx-generic/tsconfig.lib.json",
            "project": "projects/signature-it/ngx-generic/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/signature-it/ngx-generic/src/test.ts",
            "tsConfig": "projects/signature-it/ngx-generic/tsconfig.spec.json",
            "karmaConfig": "projects/signature-it/ngx-generic/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/signature-it/ngx-generic/tsconfig.lib.json",
              "projects/signature-it/ngx-generic/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "@signature-it/ngx-catalogue": {
      "root": "projects/signature-it/ngx-catalogue",
      "sourceRoot": "projects/signature-it/ngx-catalogue/src",
      "projectType": "library",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/signature-it/ngx-catalogue/tsconfig.lib.json",
            "project": "projects/signature-it/ngx-catalogue/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/signature-it/ngx-catalogue/src/test.ts",
            "tsConfig": "projects/signature-it/ngx-catalogue/tsconfig.spec.json",
            "karmaConfig": "projects/signature-it/ngx-catalogue/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/signature-it/ngx-catalogue/tsconfig.lib.json",
              "projects/signature-it/ngx-catalogue/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "@signature-it/ngx-media": {
      "root": "projects/signature-it/ngx-media",
      "sourceRoot": "projects/signature-it/ngx-media/src",
      "projectType": "library",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/signature-it/ngx-media/tsconfig.lib.json",
            "project": "projects/signature-it/ngx-media/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/signature-it/ngx-media/src/test.ts",
            "tsConfig": "projects/signature-it/ngx-media/tsconfig.spec.json",
            "karmaConfig": "projects/signature-it/ngx-media/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/signature-it/ngx-media/tsconfig.lib.json",
              "projects/signature-it/ngx-media/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "@signature-it/ngx-admin": {
      "projectType": "library",
      "root": "projects/signature-it/ngx-admin",
      "sourceRoot": "projects/signature-it/ngx-admin/src",
      "prefix": "admin",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/signature-it/ngx-admin/tsconfig.lib.json",
            "project": "projects/signature-it/ngx-admin/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/signature-it/ngx-admin/src/test.ts",
            "tsConfig": "projects/signature-it/ngx-admin/tsconfig.spec.json",
            "karmaConfig": "projects/signature-it/ngx-admin/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/signature-it/ngx-admin/tsconfig.lib.json",
              "projects/signature-it/ngx-admin/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "app-generic-new"
}
