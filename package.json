{
  "name": "app-generic",
  "description": "Generic Core Application",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "prestart": "npm run build:prod && ngc",
    "start": "ts-node web/server.ts",
    "hmr": "ng serve -sm=false -o=true -hmr=true -e=hmr",
    "build-sass": "cd node_modules/node-sass && node scripts/build -f && cd ../../",
    "build:prod": "ng build -prod --vendor-chunk=false",
    "build:prod:no-hash": "npm run build:prod --output-hashing none",
    "chmod:dist": "chmod -R g+w dist",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "bower install; npm install; php composer.phar install",
    "translations-import": "cd vendor/signature/system/tools && php console.php translations:file:import && cd ../../../..",
    "client:build": "npm run translations-import && ng build && npm run chmod:dist",
    "client:build:prod": "npm run translations-import && npm run build:prod && npm run chmod:dist",
    "env:update": "./bin/env_update.sh"
  },
  "private": true,
  "dependencies": {
