{
  "name": "app-generic",
  "description": "Generic Core Application",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "prestart": "npm run build:prod && ngc",
    "start": "ts-node web/server.ts",
    "hmr": "ng serve -sm=false -o=true -hmr=true -e=hmr",
    "prebuild:ngx-generic": "./bin/pre_build.sh",
    "build:ngx-generic": "ng build @signature-it/ngx-generic",
    "build:ngx-catalogue": "ng build @signature-it/ngx-catalogue",
    "build:ngx-admin": "ng build @signature-it/ngx-admin",
    "build-sass": "cd node_modules/node-sass && node scripts/build -f && cd ../../",
    "build:prod": "rm -rf node_modules/.cache && ng build --aot --prod --vendor-chunk=false --build-optimizer",
    "build:prod:no-hash": "npm run build:prod --output-hashing none",
    "build": "ng build",
    "chmod:dist": "chmod -R g+w dist",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "npm install; php composer.phar install",
    "env:update": "./bin/env_update.sh"
  },
