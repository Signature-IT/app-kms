  "name": "app-railways",
  "description": "Generic Core Application",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "prestart": "npm run build:prod && ngc",
    "start": "ts-node web/server.ts",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "hmr": "ng serve -sm=false -o=true -hmr=true -e=hmr",
    "prebuild:ngx-generic": "cd projects/signature-it/ngx-generic && npm install && cd ../../../",
    "build:ngx-generic": "ng build @signature-it/ngx-generic",
    "postbuild:ngx-generic": "cd dist/signature-it/ngx-generic && npm install && cd ../../../",
    "prebuild:ngx-catalogue": "cd projects/signature-it/ngx-catalogue && npm install && cd ../../../",
    "build:ngx-catalogue": "ng build @signature-it/ngx-catalogue",
    "postbuild:ngx-catalogue": "cd dist/signature-it/ngx-catalogue && npm install && cd ../../../",
    "prebuild:ngx-admin": "cd projects/signature-it/ngx-admin && npm install && cd ../../../",
    "build:ngx-admin": "ng build @signature-it/ngx-admin",
    "postbuild:ngx-admin": "cd dist/signature-it/ngx-admin && npm install && cd ../../../",
    "prebuild:ngx-entity": "cd projects/signature-it/ngx-entity && npm install && cd ../../../",
    "build:ngx-entity": "ng build @signature-it/ngx-entity",
    "postbuild:ngx-entity": "cd dist/signature-it/ngx-entity && npm install && cd ../../../",
    "build-sass": "cd node_modules/node-sass && node scripts/build -f && cd ../../",
    "build:prod": "rm -rf node_modules/.cache && node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --aot --configuration production --vendor-chunk=false --build-optimizer",
    "build:prod:no-hash": "npm run build:prod --output-hashing none",
    "build": "ng build",
    "chmod:dist": "chmod -R g+w dist",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "npm install; php composer.phar install",
    "env:update": "./projects/signature-it/ngx-generic/bin/env_update.sh -s railways -e true"
  },
